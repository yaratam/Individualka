<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="62dp"
            android:layout_width="271dp"
            android:id="@+id/button2"
            motion:layout_constraintTop_toBottomOf="@+id/text3"
            android:layout_marginTop="76dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_editor_absoluteX="37dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.332"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/acc"
            motion:layout_editor_absoluteY="794dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/acc"
            android:id="@+id/textView"
            motion:layout_editor_absoluteY="794dp"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_height="47dp"
            android:layout_width="270dp"
            android:id="@+id/angry_btn"
            motion:layout_constraintTop_toBottomOf="@+id/text3"
            motion:layout_constraintBottom_toTopOf="@+id/acc"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">



    </ConstraintSet>
</MotionScene>